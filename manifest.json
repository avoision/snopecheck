{
  "manifest_version": 2,
  "name": "SnopeCheck",
  "version": "0.1",
  "permissions": [
        "http://www.snopes.com/search*"
    ],  
  "content_scripts": [
  	{
  		"matches": [
  			"<all_urls>"
  		],
      "js": ["jquery-2.2.1.min.js", "snopecheck.js"]
  	}
  ],
  "background": {
  	"scripts": ["eventPage.js"],
  	"persistent": false
  }



}